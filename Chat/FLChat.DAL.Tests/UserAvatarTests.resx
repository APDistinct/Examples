  <data name="UserAvatar_UpdateFieldInUser_TestAction.SqlScript" xml:space="preserve">
    <value>/*
test triggers UserAvatar_OnInsertUpdate_UpdDateInUser and UserAvatar_OnDelete_ClearDateInUser
which update field [AvatarUploadDate] in [Usr].[User]
*/

declare @userId uniqueidentifier;
declare @mid integer;

set @userId = (select top 1 [Id] 
from [Usr].[User] u
left join [Usr].[UserAvatar] a on u.[Id] = a.[UserId]
where a.[UserId] is null and u.[AvatarUploadDate] is null);

if @userId is null
begin
  set @userId = newid();
  insert into [Usr].[User] ([Id], [FullName]) values (@userId, 'created for test user avatars');
end

set @mid = (select top 1 [Id] from [Cfg].[MediaType] where [CanBeAvatar] = 1);
if @mid is null
  throw 51002, 'Media type for avator not found', 1;

-- #1 must be null
select [AvatarUploadDate] from [Usr].[User] where [Id] = @userId;

insert into [Usr].[UserAvatar] ([UserId], [Data], [MediaTypeId])
values (@userId, cast('123' as varbinary(max)), @mid);

-- #2 must not null
select [AvatarUploadDate] from [Usr].[User] where [Id] = @userId and [AvatarUploadDate] is not null;

update [Usr].[User] set [AvatarUploadDate] = '2010-01-01' where [Id] = @userId;
update [Usr].[UserAvatar] set [Data] = cast('1234' as varbinary(max)) where [UserId] = @userId;

-- #3 must not null
select [AvatarUploadDate],
   case when [AvatarUploadDate] &lt;&gt; cast('2010-01-01' as datetime) then 1 else 0 end
from [Usr].[User] where [Id] = @userId and [AvatarUploadDate] is not null;

delete from [Usr].[UserAvatar] where [UserId] = @userId;

-- #4 must be null
select [AvatarUploadDate] from [Usr].[User] where [Id] = @userId;</value>
  </data>
